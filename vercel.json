{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "has": [
        {
          "type": "host",
          "value": "(?!survey\\.)"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/survey-landing.html",
      "has": [
        {
          "type": "host",
          "value": "survey.theshetalks.club"
        }
      ],
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
